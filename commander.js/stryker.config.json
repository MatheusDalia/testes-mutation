{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "_comment": "This config was generated using 'stryker init'",
  "packageManager": "npm",
  "reporters": [
    "html",
    "clear-text",
    "progress",
    "json"
  ],
  "testRunner": "jest",
  "coverageAnalysis": "perTest",
  "ignoreStatic": true,
  "mutate": [
    "lib/option.js"
  ],
  "jest": {
    "projectType": "custom",
    "configFile": "jest.config.js",
    "enableFindRelatedTests": false,
    "config": {
        "testPathIgnorePatterns": [
            "/node_modules/",
            "tests/command.configureOutput.test.js",
            "tests/command.executableSubcommand.lookup.test.js"
        ]
    }
  }
}
